#include<iostream>
using namespace std;
int main(){
    string s;
    int k,n,c=0;
    cin>>n>>k;
    cin>>s;
    for(int i=0;i<n;i++){
        if(c==k)
        break;
        if(s[0]!='1'){
            s[i]='1';
            c++;
            continue;
        }
        if(s[i]!='0'){
            s[i]='0';
            c++;
        }
    }
    cout<<s;
    return 0;
    }
