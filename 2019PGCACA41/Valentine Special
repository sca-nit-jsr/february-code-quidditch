
#include <iostream>
#include<cmath>

using namespace std;

int main() {
	int n,k;
	char num[100000];
	cin >> n >> k;
	cin >> num;
	int i=0;
	while(i<k&&i<n)
	{
	  if(i==0&&num[i]=='1')
	   k++;
	  else if(i==0&&num[i]!='1')
	   num[i]='1';
	  else if(num[i]=='0')
	   k++;
	  else
	   num[i]='0';
	   i++;
	}
	cout<<num;
	 
	return 0;
	
}

